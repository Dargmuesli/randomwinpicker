{
    "Owner": "dargmuesli",
    "RegistryAddress": {
        "Name": "registry",
        "Hostname": "localhost",
        "Port": "5000"
    },
    "ComposeFile": {
        "Name": "stack.yml",
        "Content": {
            "version": "3.6",
            "services": {
                "adminer": {
                    "image": "adminer",
                    "deploy": {
                        "labels": [
                            "traefik.enable=true",
                            "traefik.frontend.rule=Host:adminer.randomwinpicker.test",
                            "traefik.port=8080"
                        ]
                    },
                    "volumes": [
                        "./production/adminer/adminer.css:/var/www/html/adminer.css"
                    ]
                },
                "randomwinpicker-de": {
                    "image": "localhost:5000/dargmuesli/randomwinpicker",
                    "deploy": {
                        "labels": [
                            "traefik.enable=true",
                            "traefik.frontend.rule=Host:randomwinpicker.test,www.randomwinpicker.test",
                            "traefik.port=443",
                            "traefik.protocol=https"
                        ]
                    },
                    "volumes": [
                        "./dist/randomwinpicker:/var/www/randomwinpicker",
                        "./docker/randomwinpicker/certificates:/etc/ssl/certificates"
                    ]
                },
                "postgres": {
                    "image": "postgres",
                    "environment": {
                        "POSTGRES_DB_FILE": "/run/secrets/postgres_db",
                        "POSTGRES_PASSWORD_FILE": "/run/secrets/postgres_password",
                        "POSTGRES_USER_FILE": "/run/secrets/postgres_user"
                    },
                    "secrets": [
                        "postgres_db",
                        "postgres_password",
                        "postgres_user"
                    ],
                    "volumes": [
                        "postgres-data:/var/lib/postgresql/data"
                    ]
                },
                "traefik": {
                    "image": "traefik",
                    "ports": [
                        {
                            "target": 80,
                            "published": 80,
                            "protocol": "tcp",
                            "mode": "host"
                        },
                        {
                            "target": 443,
                            "published": 443,
                            "protocol": "tcp",
                            "mode": "host"
                        }
                    ],
                    "deploy": {
                        "labels": [
                            "traefik.docker.network=randomwinpicker-de_default",
                            "traefik.enable=true",
                            "traefik.frontend.rule=Host:traefik.randomwinpicker.test",
                            "traefik.port=8080"
                        ],
                        "mode": "global",
                        "placement": {
                            "constraints": [
                                "node.role == manager"
                            ]
                        }
                    },
                    "command": [
                        "--api",
                        "--defaultentrypoints=http,https",
                        "--docker",
                        "--docker.domain=randomwinpicker.de",
                        "--docker.exposedByDefault=false",
                        "--docker.swarmMode=true",
                        "--entryPoints=Name:http Address::80 Redirect.EntryPoint:https",
                        "--entryPoints=Name:https Address::443 TLS:/etc/ssl/certificates/randomwinpicker.crt,/etc/ssl/certificates/randomwinpicker.key Compress:true",
                        "--insecureSkipVerify=true"
                    ],
                    "volumes": [
                        "/var/run/docker.sock:/var/run/docker.sock",
                        "./docker/randomwinpicker/certificates:/etc/ssl/certificates"
                    ]
                }
            },
            "secrets": {
                "postgres_db": {
                    "file": "./docker/secrets/postgres_db"
                },
                "postgres_password": {
                    "file": "./docker/secrets/postgres_password"
                },
                "postgres_user": {
                    "file": "./docker/secrets/postgres_user"
                }
            },
            "volumes": {
                "postgres-data": {}
            }
        }
    }
}
